[Unit]
Description=Wstunnel Client
After=network-online.target

[Service]
Type=simple
# This ensures we don't have the proxy issues from earlier
UnsetEnvironment=HTTP_PROXY HTTPS_PROXY http_proxy https_proxy
ExecStart=/usr/local/bin/wstunnel client -L udp://127.0.0.100:16384:127.0.0.1:16384 wss://ai.zxaicc.com
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
